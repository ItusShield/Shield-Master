config uhttpd 'main'
	list listen_http '0.0.0.0:80'
	list listen_https '0.0.0.0:443'
	option home '/www'
	option rfc1918_filter '1'
	option max_requests '3'
	option max_connections '100'
	option cert '/etc/uhttpd.crt'
	option key '/etc/uhttpd.key'
	option cgi_prefix '/cgi-bin'
	option script_timeout '600'
	option network_timeout '300'
	option http_keepalive '20'
	option tcp_keepalive '1'
	option ubus_prefix '/ubus'

config cert 'px5g'
	option days '730'
	option bits '2048'
	option country 'US'
	option state 'California'
	option location 'SiliconValley'
	option commonname 'Shield'

config uhttpd 'Itusfilter'
	list listen_http '0.0.0.0:88'
	option home '/www/block/'
	option rfc1918_filter '1'
	option error_page '/itus_error_8.png'
	option index_page 'index.html'
	option max_requests '3'
	option max_connections '100'
	option network_timeout '300'
	option http_keepalive '20'
	option tcp_keepalive '1'
	option ubus_prefix '/ubus'
